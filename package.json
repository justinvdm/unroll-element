{
  "name": "unroll-element",
  "version": "0.3.4",
  "description": "unroll react elements using your own host element resolver",
  "license": "MIT",
  "repository": "justinvdm/unroll-element",
  "type": "commonjs",
  "main": "index.js",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.js"
  },
  "author": {
    "name": "justinvdm",
    "email": "justnvdm@gmail.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "otr build",
    "lint": "otr lint",
    "format": "otr format",
    "test": "otr test",
    "release": "otr release",
    "typetest": "otr typetest",
    "checks": "yarn lint && yarn typetest && yarn test"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "dist/*"
  ],
  "keywords": [
    "unroll",
    "resolve",
    "react",
    "element",
    "jsx"
  ],
  "oftherivier": {
    "type": "es5",
    "src": "index.js",
    "libName": "unrollElement"
  },
  "renovate": {
    "extends": [
      "github>oftherivier/tools"
    ]
  },
  "ava": {
    "babel": true
  },
  "dependencies": {
    "@types/react": "^16.9.35"
  },
  "devDependencies": {
    "@oftherivier/tools": "0.1.37",
    "react": "16.13.1"
  },
  "peerDependencies": {
    "react": "*"
  }
}
